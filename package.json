{
  "name": "budget-tracker-frontend",
  "version": "1.0.0",
  "description": "Frontend for the budget tracker app.",
  "author": "fizwidget@gmail.com",
  "license": "MIT",
  "browserslist": [
    "Chrome 78"
  ],
  "scripts": {
    "start": "yarn relay && parcel serve src/index.html --open",
    "build": "yarn relay && yarn typecheck && yarn lint && parcel build src/index.html",
    "clean": "rm -rf .parcel-cache dist node_modules",
    "relay": "relay-compiler --schema schema.graphql --src ./src/ --language typescript --watchman false $@",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "babel-plugin-macros": "^2.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^0.0.0-experimental-183bdd28",
    "relay-runtime": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-relay": "^7.0.8",
    "@types/relay-runtime": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-plugin-relay": "^10.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-relay": "^1.8.1",
    "graphql": "^15.3.0",
    "parcel": "^2.0.0-beta.1",
    "prettier": "2.1.1",
    "relay-compiler": "^10.0.1",
    "relay-compiler-language-typescript": "^13.0.1",
    "typescript": "^4.0.2"
  }
}
