{
  "name": "budget-tracker-frontend",
  "version": "1.0.0",
  "description": "Frontend for the budget tracker app.",
  "scripts": {
    "start": "yarn relay && parcel src/index.html --open",
    "build": "yarn relay && yarn typecheck && parcel build src/index.html",
    "relay": "yarn run relay-compiler --schema schema.graphql --src ./src/ --watchman false $@",
    "typecheck": "tsc --noEmit"
  },
  "author": "fizwidget@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-relay": "^7.0.8",
    "@types/relay-runtime": "^10.0.3",
    "babel-plugin-relay": "^10.0.1",
    "graphql": "^15.3.0",
    "parcel-bundler": "^1.12.4",
    "relay-compiler": "^10.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^0.0.0-experimental-183bdd28",
    "relay-runtime": "^10.0.1"
  }
}
